<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/10/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/10/modelMap https://cayenne.apache.org/schema/10/modelMap.xsd"
	 project-version="10">
	<property name="defaultPackage" value="org.example.cayenne.persistent."/>
	<db-entity name="ARTIST">
		<db-attribute name="DATE_OF_BIRTH_DATE" type="DATE"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="NAME" type="VARCHAR" length="200"/>
	</db-entity>
	<db-entity name="GALARY">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="NAME" type="VARCHAR" length="200"/>
	</db-entity>
	<db-entity name="PAITING">
		<db-attribute name="ARTIST_ID" type="INTEGER"/>
		<db-attribute name="GALLERY_ID" type="INTEGER"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="NAME" type="VARCHAR" length="200"/>
	</db-entity>
	<obj-entity name="Artist" className="org.example.cayenne.persistent.Artist" dbEntityName="ARTIST">
		<obj-attribute name="dateOfBirthDate" type="java.time.LocalDate" db-attribute-path="DATE_OF_BIRTH_DATE"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="NAME"/>
	</obj-entity>
	<obj-entity name="Galary" className="org.example.cayenne.persistent.Galary" dbEntityName="GALARY">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="NAME"/>
	</obj-entity>
	<obj-entity name="Paiting" className="org.example.cayenne.persistent.Paiting" dbEntityName="PAITING">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="NAME"/>
	</obj-entity>
	<db-relationship name="paitings" source="ARTIST" target="PAITING" toMany="true">
		<db-attribute-pair source="ID" target="ARTIST_ID"/>
	</db-relationship>
	<db-relationship name="paitings" source="GALARY" target="PAITING" toMany="true">
		<db-attribute-pair source="ID" target="GALLERY_ID"/>
	</db-relationship>
	<db-relationship name="artist" source="PAITING" target="ARTIST">
		<db-attribute-pair source="ARTIST_ID" target="ID"/>
	</db-relationship>
	<db-relationship name="galary" source="PAITING" target="GALARY">
		<db-attribute-pair source="GALLERY_ID" target="ID"/>
	</db-relationship>
	<obj-relationship name="paitings" source="Artist" target="Paiting" deleteRule="Deny" db-relationship-path="paitings"/>
	<obj-relationship name="paitings" source="Galary" target="Paiting" deleteRule="Deny" db-relationship-path="paitings"/>
	<obj-relationship name="artist" source="Paiting" target="Artist" deleteRule="Nullify" db-relationship-path="artist"/>
	<obj-relationship name="galary" source="Paiting" target="Galary" deleteRule="Nullify" db-relationship-path="galary"/>
</data-map>
